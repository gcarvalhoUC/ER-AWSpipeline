{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Resources": {
    "MyDatabase": {
      "Type": "AWS::RDS::DBInstance",
      "Properties": {
        "DBName": "mydatabase",
        "DBInstanceClass": "db.t2.micro",
        "Engine": "MySQL",
        "AllocatedStorage": 5,
        "MasterUsername": "myusername",
        "MasterUserPassword": "mypassword",
        "DBSubnetGroupName": "mysubnetgroup",
        "VPCSecurityGroups": [
          "mysecuritygroup"
        ],
        "DBParameterGroupName": "default.mysql5.7",
        "Tags": [
          {
            "Key": "Name",
            "Value": "MyDatabase"
          }
        ]
      }
    },
    "MyDatabaseSchema": {
      "Type": "AWS::RDS::DBSchema",
      "Properties": {
        "DBName": "mydatabase",
        "Schema": {
          "Tables": [
            {
              "Name": "MyTable",
              "Columns": [
                {
                  "Name": "id",
                  "Type": "int"
                },
                {
                  "Name": "name",
                  "Type": "string"
                },
                {
                  "Name": "age",
                  "Type": "int"
                }
              ]
            }
          ]
        }
      }
    }
  }
}
